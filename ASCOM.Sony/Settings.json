  // --------------------------------------------------------------------------------------------------
  // not advisable to change these unless you're having problems with the driver doing things too fast.
  // --------------------------------------------------------------------------------------------------
  "Delays": {
    "FileAccessRetry": 500, // delay between file image file access retries if the first fails
    "FileAccessTimer": 1000, // delay after verifying we can access the file, before reading the file
    "RemoteWindowRetry": 500, // delay between checking if the Remote app has loaded
    "NoWindowRetry": 500, // delay when no active remote window is found, can happen after selecting a camera from camera select screen
    "StateMachineRetry": 500, // delay between window state machine states
    "CannotCreateFolderDelay": 10000, // user needs to create a new folder to save the photos if the current one cannot be accessed
    "SettingVerification": 1000, // wait for UI to display the current camera setting, if too short will cause driver to get into infinite settings changing loop
    "SettingUpdate": 200 // delay after pressing ISO/SS increase/decrease before pressing again, can be done much faster than the UI can update.
  },

